@model MVC8amSuperStarBatch.Models.EmployeeDeptModel
@{
    ViewBag.Title = "getMultipleModels";
}

<h2>getMultipleModels</h2>



<table>
    <tr>
        <th>EmpId</th>
        <th>EmpName</th>
        <th>EmpSalary</th>
    </tr>
    @foreach (var item in Model.Emp)
    {
        <tr>
            <td>@item.EmpId</td>
            <td>@item.EmpName</td>
            <td>@item.EmpSalary</td>
        </tr>
    }
</table>


<br />

<table>
    <tr>
        <th>DeptId</th>
        <th>DeptName</th>
         
    </tr>
    @foreach (var item in Model.Dept)
    {
        <tr>
            <td>@item.DeptId</td>
            <td>@item.DeptName</td>
            
        </tr>
    }
</table>