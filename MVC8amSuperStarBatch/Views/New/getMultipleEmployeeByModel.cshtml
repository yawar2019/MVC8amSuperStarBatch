@model List<MVC8amSuperStarBatch.Models.EmployeeModel>
@{
    ViewBag.Title = "getMultipleEmployeeByModel";
}

<h2>getMultipleEmployeeByModel</h2>

<table>
    <tr>
        <th>EmpId</th>
        <th>EmpName</th>
        <th>EmpSalary</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.EmpId</td>
            <td>@item.EmpName</td>
            <td>@item.EmpSalary</td>
        </tr>
    }
</table>